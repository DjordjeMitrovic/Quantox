<% include ../../partials/header %>
<% include ../../partials/menu %>


<% if(user.activated==0){ %>

<div> Please confirm your account through link we sent you on your email. </div>

<%} else{%>
<% include ../../partials/options %>

<% if(typeof activities[0] != 'undefined' ){%>
<h1>Displaying Edit History for ticket
    <%=activities[0].title%>
</h1>
<table class="table mt-5">
    <thead>
        <tr>

            <th>Username</th>

            <th>Operation</th>


        </tr>
    </thead>
    <tbody>
        <% activities.forEach(function(activity) { %>
        <tr>
            <td>
                <%=activity.username%>
            </td>

            <td>

                <%=activity.activity%>


            </td>



        </tr>
        <% }); %>
    </tbody>
</table>
<%} else{%>

<h1>No Edit History to display</h1>
<%}%>
<% }%>
<% include ../../partials/footer %>